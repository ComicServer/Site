function setFav(clicked_id) {
const name = localStorage.getItem(clicked_id + "s")
if(name === null) {
let newname = prompt("Enter the comic\'s name", "");
if (newname !== null | "") {
localStorage.setItem(clicked_id + "s", newname)
document.getElementById(clicked_id).innerHTML = newname;   document.getElementById(clicked_id).setAttribute("onClick", `window.location='/search/${newname}'`);
  }
} else {
return;
}
}
function delFav() {
let oldname = prompt("Enter the comic\'s slot # for deletion (1-8)", "");
