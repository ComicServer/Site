<script>
function themeAuto() {
const darkThemeMq = window.matchMedia("(prefers-color-scheme: dark)");
darkThemeMq.addListener(e => {
if (e.matches) {
themeBlackout()
} else {
themeBasic()
}
})
if (darkThemeMq.matches === true) {
themeBlackout()
} else {
themeBlackout()
}
}
var r = document.querySelector(':root');
function themeBirch() {
  r.style.setProperty('--batext', 'white');
  r.style.setProperty('--basic', '#704F32');
  r.style.setProperty('--back', "url('https://old.comicserver.org/img/wood.png')")
}
function themeBlast() {
  r.style.setProperty('--batext', 'white');
  r.style.setProperty('--basic', '#5e17eb');
  r.style.setProperty('--back', "url('https://old.comicserver.org/img/Coolio.png')")
}
function themeWelDays() {
  r.style.setProperty('--batext', 'white');
  r.style.setProperty('--basic', '#004242');
  r.style.setProperty('--ba', 'rgba(255, 255, 255, 0.5)');
  snowStorm.toggleSnow()
}
function themeBlackout() {
  r.style.setProperty('--batext', 'RGBA(255, 255, 255, .7)');
  r.style.setProperty('--ba', 'RGBA(245, 248, 252, .5)');
  r.style.setProperty('--basic', '#2d2d2d');
  r.style.setProperty('--bt', null);
}
function themeBasic() {
  r.style.setProperty('--batext', 'RGBA(0, 0, 0, .7)');
  r.style.setProperty('--basic', '#E3EBF8');
  r.style.setProperty('--ba', 'RGBA(0, 0, 0, .7)');
  
}
function themeCustom() {
  var customtext = localStorage.getItem('text')
  if(customback === null) customback = "#fff"
  var customback = localStorage.getItem('back')
  if(customtext === null) customtext = "#fff"
  var customsec = localStorage.getItem('sec')
  if(customsec === null) customsec = "#fff"
  r.style.setProperty('--batext', customtext);
  r.style.setProperty('--basic', customback);
  r.style.setProperty('--ba', customsec);
}
if(localStorage.getItem('theme') === 'blackout') {
themeBlackout();
} else if(localStorage.getItem('theme') === 'basic') {
themeBasic();
} else if(localStorage.getItem('theme') === 'auto') {
themeAuto();
} else if(localStorage.getItem('theme') === 'custom') {
themeCustom();
} else if(localStorage.getItem('theme') === 'weldays') {
themeWelDays();
} else {
themeBlackout();
}
  
const hex = r.style.getPropertyValue('--ba')

const red = parseInt(hex.substring(1, 3), 16);
const green = parseInt(hex.substring(3, 5), 16);
const blue = parseInt(hex.substring(5, 7), 16);
const stringg = `${red}, ${green}, ${blue}`
const string = stringg.split(',')
r.style.setProperty('--yo', string);
</script>
