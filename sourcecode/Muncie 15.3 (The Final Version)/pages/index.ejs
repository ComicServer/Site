          <div class="preloader-bar"></div> 
      </div>
</div>
</div><script src="//code.jquery.com/jquery.min.js"></script>
<script src="https://www.jqueryscript.net/demo/jQuery-Plugin-To-Preload-Images-And-Web-Content-sPreLoader/js/sPreloader.js"></script>
<script>
var sswb = sessionStorage.getItem('wb')
if(sswb === false) {
// Greeting List
const tz = Intl.DateTimeFormat().resolvedOptions().timeZone;
const greetings = ["Good Morning,", "Good Afternoon,", "Good Evening,"]
const ddd = new Date()
ddd.toLocaleString('en-US', { timeZone: tz })
const hrs = ddd.getHours()
if (hrs > 00 && hrs < 11) {
document.getElementById("wbs").innerHTML = greetings[0];
} else if (hrs > 11 && hrs < 16) {
document.getElementById("wbs").innerHTML = greetings[1];
} else if (hrs > 16 && hrs < 24) {
document.getElementById("wbs").innerHTML = greetings[2];
}
$('body').jpreLoader({
  preMainSection: '#main-preloader',
  prePerText: '.preloader-percentage-text',
  preBar: '.preloader-bar',
  showPercentage: false,
  debugMode: false,
})
sessionStorage.setItem('wb', true)
if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
document.getElementById('ssuname').style.fontSize = "3em";
document.getElementById('ssuname').style.width = "100%";
}
} else if(sswb === null) {
// Greeting List
const tz = Intl.DateTimeFormat().resolvedOptions().timeZone;
const greetings = ["Good Morning,", "Good Afternoon,", "Good Evening,"]
const ddd = new Date()
ddd.toLocaleString('en-US', { timeZone: tz })
const hrs = ddd.getHours()
if (hrs > 00 && hrs < 11) {
document.getElementById("wbs").innerHTML = greetings[0];
} else if (hrs > 11 && hrs < 16) {
document.getElementById("wbs").innerHTML = greetings[1];
} else if (hrs > 16 && hrs < 24) {
document.getElementById("wbs").innerHTML = greetings[2];
}
$('body').jpreLoader({

  // default CSS classes for the preloader
  preMainSection:     '#main-preloader',
  prePerText:         '.preloader-percentage-text',
  preBar:             '.preloader-bar',

  // show percentage value
  showPercentage: false,

  // enable debug mode
  debugMode: false,

  // function  
})
sessionStorage.setItem('wb', true)
if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
document.getElementById('ssuname').style.fontSize = "3em";
document.getElementById('ssuname').style.width = "100%";
}
} else {
document.getElementById('main-preloader').style.display = "none"
}
  function forceRefresh() {
  let ts = Date.now();
  let da = new Date(ts);
  utc = da.getTime() + da.getTimezoneOffset() * 60000;
  offset = -6;
  const date = new Date(utc + 3600000 * offset);
  const dayed = date.getDate()
  localStorage.removeItem('sheet-<%= uname %>' + dayed)
  window.location.reload()
  }
</script>