<script>
function setComic() {
const stco = prompt("Please set a comic to start with when you use ComicServer");
var startcomic = localStorage.getItem("stco")
if(stco === null || "") {
return;
} else if(startcomic !== null) {
localStorage.setItem("stco", stco)
window.location.replace("/");
}
}
