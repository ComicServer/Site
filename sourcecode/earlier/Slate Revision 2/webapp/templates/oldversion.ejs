  <input type="text" name="SearchQuery" placeholder="Search the webapp..." required="">
    <input type="submit" name="submitbutton" class="btn btn-success hidden" id="srch-index" value="Search" onclick="srch(SearchQuery);">
</form>
<script>
function srch(element)
{
if(element.value === ""){
return;
}
if(element.value === " "){
return;
}
if(element.value === "‎"){
return;
}
  var urlstring = `https://app.comicserver.org/search/${element.value}`;
  window.location.replace(urlstring);
}

function submitcheck(element)
{
  return false;
}
</script>
