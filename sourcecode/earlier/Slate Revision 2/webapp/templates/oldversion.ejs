    <script>
    function setBtnSettings(yesno){
       if(yesno === "yes") {
       var element = document.getElementById("button-hidden");
       element.classList.remove("hidden");
       var elementt = document.getElementById("button-hiddenn");
       elementt.classList.remove("hidden");
       var elementtt = document.getElementById("button-hiddennn");
       elementtt.classList.remove("hidden");
       var elementttt = document.getElementById("srch-index");
       elementttt.classList.remove("hidden");
       }
       if(yesno === "no") {
       var element = document.getElementById("button-hidden");
       element.classList.add("hidden");
       var elementt = document.getElementById("button-hiddenn");
       elementt.classList.add("hidden");
       var elementtt = document.getElementById("button-hiddennn");
       elementtt.classList.add("hidden");
       var elementttt = document.getElementById("srch-index");
       elementttt.classList.add("hidden");
       }
       }
    </script>
